<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="weatherGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:0.3" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="#f8fafc"/>
  
  <!-- Browser chrome -->
  <rect width="600" height="50" fill="#e2e8f0"/>
  <circle cx="20" cy="25" r="6" fill="#ef4444"/>
  <circle cx="40" cy="25" r="6" fill="#f59e0b"/>
  <circle cx="60" cy="25" r="6" fill="#10b981"/>
  <rect x="100" y="15" width="400" height="20" rx="10" fill="#f1f5f9"/>
  
  <!-- Content area -->
  <rect x="20" y="70" width="560" height="310" fill="white" stroke="#1e40af" stroke-width="1" opacity="0.5"/>
  <rect x="20" y="70" width="560" height="60" fill="url(#weatherGradient)"/>
  
  <!-- Header -->
  <text x="300" y="105" font-family="Arial, sans-serif" font-size="24" font-weight="bold" 
        text-anchor="middle" fill="#1e40af">Weather Dashboard</text>
  
  <!-- Weather cards -->
  <rect x="40" y="150" width="120" height="80" fill="#1e40af" opacity="0.1" rx="8"/>
  <circle cx="100" cy="175" r="10" fill="#f59e0b"/>
  <text x="100" y="200" font-family="Arial, sans-serif" font-size="12" 
        text-anchor="middle" fill="#1e40af">24°C</text>
  <text x="100" y="215" font-family="Arial, sans-serif" font-size="10" 
        text-anchor="middle" fill="#1e40af">Sunny</text>
  
  <rect x="180" y="150" width="120" height="80" fill="#64748b" opacity="0.1" rx="8"/>
  <circle cx="240" cy="175" r="10" fill="#64748b"/>
  <text x="240" y="200" font-family="Arial, sans-serif" font-size="12" 
        text-anchor="middle" fill="#64748b">18°C</text>
  <text x="240" y="215" font-family="Arial, sans-serif" font-size="10" 
        text-anchor="middle" fill="#64748b">Cloudy</text>
  
  <!-- Chart area -->
  <rect x="340" y="150" width="220" height="120" fill="#1e40af" opacity="0.05" rx="8"/>
  <polyline points="350,220 380,200 410,190 440,210 470,180 500,170 530,160" 
            fill="none" stroke="#1e40af" stroke-width="3"/>
  <text x="440" y="250" font-family="Arial, sans-serif" font-size="10" 
        text-anchor="middle" fill="#1e40af">7-Day Forecast</text>
  
  <!-- Map area -->
  <rect x="40" y="250" width="260" height="100" fill="#06d6a0" opacity="0.1" rx="8"/>
  <circle cx="150" cy="300" r="20" fill="#06d6a0" opacity="0.3"/>
  <text x="150" y="305" font-family="Arial, sans-serif" font-size="10" 
        text-anchor="middle" fill="#06d6a0">Location</text>
</svg>